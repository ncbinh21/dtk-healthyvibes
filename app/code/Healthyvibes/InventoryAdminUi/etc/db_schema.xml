<?xml version="1.0"?>

<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="inventory_source">
        <column xsi:type="int" name="city_id" unsigned="true" nullable="true" identity="false"/>
        <column xsi:type="int" name="ward_id" unsigned="true" nullable="true" identity="false"/>
        <column xsi:type="varchar" name="ward" nullable="true" length="255"/>
        <column xsi:type="int" name="domain_id" unsigned="true" nullable="true" comment="Domain Id"/>
        <column xsi:type="varchar" name="shop_id_ghn" length="255" nullable="true" comment="Shop Id (GHN)"/>
    </table>
</schema>
